<%- include('../layout/Header') %>
    <%- include('../layout/Navbar') %>
        <div class="add-container">
            <div class="add">
                <p>Add to Diary</p>
                <a href="/add">ADD</a>
            </div>

            <div class="diary">
                <% data.forEach((diary)=>{%>
                    <div class="diary-container">
                        <h3>
                            <%= diary.title %>
    
                        </h3>
                        <p>
                            <%= diary.date%>
                        </p>
                        <a href="/diary/<%= diary.id %>">More..</a>
                    </div>
               <%}) %>
            </div>

        </div>
        

<%- include('../layout/Footer') %>
